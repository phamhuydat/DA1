<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/App.Web.styles.css" asp-append-version="true" />
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-DJ5D8BL4TW"></script>
    <link href="~/clienttle/css/style.css" rel="stylesheet" asp-append-version="true" />

    @RenderSection("head", required: false)
</head>
<body>
    <div class="master-wrapper-page">
        <partial name="_header.cshtml" />
         @RenderBody()
         <partial name="_footer.cshtml" />
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
