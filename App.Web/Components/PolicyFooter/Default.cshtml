@model IEnumerable<App.Web.ViewModels.Policy.ListPolicyFooterVM>

@if (Model.Count() > 0)
{
	foreach (var item in Model)
	{
		if (!item.Title.IsNullOrEmpty())
		{
			<li>
				<a target="_blank" 
				   asp-area=""
				   asp-controller="AppPolicy"
				   asp-action="PolicyDetail"
				   asp-route-slug="@item.Slug">
					@item.Title
				</a>
			</li>
		}
	}
}