@model IEnumerable<IGrouping<string, App.Web.ViewModels.AppBranch.ListBranchVM>>

@if (Model.Count() > 0)
{
	foreach (var item in Model)
	{
		<tr>
			<td style="width: 20px;"><i class="fa fa-map-marker"></i></td>
			<td>
				@if (item.Key == null)
				{
					<b>Chi nhánh khác:</b>
				}
				else
				{
					<b>@item.Key:</b>
				}
				<br>
				@foreach (var item2 in item)
				{
					<span>@item2.Address</span><br>
				}
			</td>
		</tr>
	}
}